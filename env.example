# Configuration Railway Database
# Copiez ce fichier vers .env.local et remplissez avec vos vraies valeurs

# Railway MySQL Database
RAILWAY_DB_HOST=containers-us-west-xxx.railway.app
RAILWAY_DB_PORT=3306
RAILWAY_DB_USER=root
RAILWAY_DB_PASSWORD=your_railway_password
RAILWAY_DB_NAME=railway
RAILWAY_DB_SSL=true

# Variables Vite (pour le frontend)
VITE_DB_HOST=containers-us-west-xxx.railway.app
VITE_DB_PORT=3306
VITE_DB_USER=root
VITE_DB_PASSWORD=your_railway_password
VITE_DB_NAME=railway
VITE_DB_SSL=true

# Configuration de l'application
VITE_PROJECT_NAME=MADON Management Suite
VITE_API_URL=https://your-vercel-app.vercel.app/api